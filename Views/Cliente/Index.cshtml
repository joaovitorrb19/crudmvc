@model List<ClienteModel>

<table class="table table-dark">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome do Cliente:</th>
            <th>Email do Cliente</th>
            <th>CPF do cliente</th>
            <th>Idade do Cliente</th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in @Model)
        {
            <tr>
                <td>@item.ClienteId</td>
                <td>@item.NomeCliente</td>
                <td>@item.EmailCliente</td>
                <td>@item.CPFCliente</td>
                <td>@((int.Parse(DateTime.Now.ToString("yyyymmdd")) - int.Parse(item.DataNascimento.ToString("yyyymmdd")))/10000) Anos de idade</td>
                <td>
                    <a class="btn btn-primary">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-check2" viewBox="0 0 16 16">
                        <path d="M13.854 3.646a.5.5 0 0 1 0 .708l-7 7a.5.5 0 0 1-.708 0l-3.5-3.5a.5.5 0 1 1 .708-.708L6.5 10.293l6.646-6.647a.5.5 0 0 1 .708 0z"/>
                        </svg>
                    </a>
                    <a class="btn btn-primary" asp-controller="Endereco" asp-action="Index" asp-route-id="@item.ClienteId"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-card-list" viewBox="0 0 16 16">
                                               <path d="M14.5 3a.5.5 0 0 1 .5.5v9a.5.5 0 0 1-.5.5h-13a.5.5 0 0 1-.5-.5v-9a.5.5 0 0 1 .5-.5h13zm-13-1A1.5 1.5 0 0 0 0 3.5v9A1.5 1.5 0 0 0 1.5 14h13a1.5 1.5 0 0 0 1.5-1.5v-9A1.5 1.5 0 0 0 14.5 2h-13z"/>
                                               <path d="M5 8a.5.5 0 0 1 .5-.5h7a.5.5 0 0 1 0 1h-7A.5.5 0 0 1 5 8zm0-2.5a.5.5 0 0 1 .5-.5h7a.5.5 0 0 1 0 1h-7a.5.5 0 0 1-.5-.5zm0 5a.5.5 0 0 1 .5-.5h7a.5.5 0 0 1 0 1h-7a.5.5 0 0 1-.5-.5zm-1-5a.5.5 0 1 1-1 0 .5.5 0 0 1 1 0zM4 8a.5.5 0 1 1-1 0 .5.5 0 0 1 1 0zm0 2.5a.5.5 0 1 1-1 0 .5.5 0 0 1 1 0z"/>
                                               </svg></a>
                    <a class="btn btn-primary" asp-controller="Cliente" asp-action="Cadastrar" asp-route-id="@item.ClienteId">Editar</a>
                    <a class="btn btn-primary" asp-controller="Cliente" asp-action="Excluir" asp-route-id="@item.ClienteId">Excluir</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<a class="btn btn-primary" asp-controller="Cliente" asp-action="Cadastrar">Adicionar</a>